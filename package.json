{
  "name": "typescript-blockchain",
  "version": "1.0.0",
  "description": "Simple blockchain in Typescript",
  "main": "index.js",
  "scripts": {
    "start": "npm run node",
    "build": "tsc -p .",
    "node": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' dev/networkNode.js",
    "node_1": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' dev/networkNode.js 3001 http://localhost:3001",
    "node_2": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' dev/networkNode.js 3002 http://localhost:3002",
    "node_3": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' dev/networkNode.js 3003 http://localhost:3003",
    "node_4": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' dev/networkNode.js 3004 http://localhost:3004",
    "node_5": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' dev/networkNode.js 3005 http://localhost:3005"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/uuid": "^3.4.4",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "nodemon": "^1.18.9",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "sha256": "^0.2.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.15",
    "typescript": "^3.2.2"
  }
}
